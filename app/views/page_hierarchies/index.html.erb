<p id="notice"><%= notice %></p>

<h1>Listing Page Hierarchies</h1>

<table>
  <thead>
    <tr>
      <th>Page</th>
      <th>Parent</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @page_hierarchies.each do |page_hierarchy| %>
      <tr>
        <td><%= page_hierarchy.page_id %></td>
        <td><%= page_hierarchy.parent_id %></td>
        <td><%= link_to 'Show', page_hierarchy %></td>
        <td><%= link_to 'Edit', edit_page_hierarchy_path(page_hierarchy) %></td>
        <td><%= link_to 'Destroy', page_hierarchy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Page hierarchy', new_page_hierarchy_path %>
